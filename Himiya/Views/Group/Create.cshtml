
@{
    ViewBag.Title = "Створити нову групу тестів";
    Layout = "~/Views/Admin/_LayoutPageAdmin.cshtml";
}

    <div class="form-wrapper">
        @using (Html.BeginForm())
        {
            <header class="table-caption clear-float">
                <h3>@ViewBag.Title</h3>
            </header>
            <input type="text" name="Name" placeholder="Ім'я групи">
            <div class="table-footer clear-float">
                <input type="submit" value="Створити">
                <a class="cancel-btn" href="@ViewBag.BackLink">Відмінити</a>
            </div>
        }
    </div>

